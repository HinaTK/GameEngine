PWD 				= /mnt/hgfs/GameEngine/lib/src
AR           		= ar cqs
CXX           		= g++
INCPATH			= -I /mnt/hgfs/GameEngine/lib/../ -I /mnt/hgfs/GameEngine/lib/include -I /mnt/hgfs/GameEngine/lib/include/frame
BINDIR				= /mnt/hgfs/GameEngine/lib/linux/
OBJDIR				= /mnt/hgfs/GameEngine/lib/linux/obj/
BIN 				= libFrame.a
SOURCES			= /mnt/hgfs/GameEngine/lib/src/frame/accepter.cpp /mnt/hgfs/GameEngine/lib/src/frame/baselistener.cpp /mnt/hgfs/GameEngine/lib/src/frame/bufmanager.cpp /mnt/hgfs/GameEngine/lib/src/frame/frame.cpp /mnt/hgfs/GameEngine/lib/src/frame/handshaker.cpp /mnt/hgfs/GameEngine/lib/src/frame/listener.cpp /mnt/hgfs/GameEngine/lib/src/frame/message.cpp /mnt/hgfs/GameEngine/lib/src/frame/netcommon.cpp /mnt/hgfs/GameEngine/lib/src/frame/nethandler.cpp /mnt/hgfs/GameEngine/lib/src/frame/netmanager.cpp /mnt/hgfs/GameEngine/lib/src/frame/netuser.cpp /mnt/hgfs/GameEngine/lib/src/frame/webaccepter.cpp /mnt/hgfs/GameEngine/lib/src/frame/weblistener.cpp /mnt/hgfs/GameEngine/lib/src/frame/websocket/base64.cpp /mnt/hgfs/GameEngine/lib/src/frame/websocket/dataframeheader.cpp /mnt/hgfs/GameEngine/lib/src/frame/websocket/sha1.cpp 
SOURCEFILES		= $(notdir $(SOURCES))
OBJECTS			= $(addprefix $(OBJDIR), $(patsubst %.cpp,%.o,$(SOURCEFILES)))
FLAGS				= -fpermissive
ARFLAGS			= 

all:makedir $(OBJECTS)
	$(AR) $(ARFLAGS) $(BIN) $(OBJECTS);

makedir:
	$(shell if [ -n "$(OBJDIR)" -a ! -e "$(OBJDIR)" ];then mkdir -p $(OBJDIR); fi)
	$(shell if [ -n "$(BINDIR)" -a ! -e "$(BINDIR)" ];then mkdir -p $(BINDIR); fi)

/mnt/hgfs/GameEngine/lib/linux/obj/accepter.o:/mnt/hgfs/GameEngine/lib/src/frame/accepter.cpp
	rm -f $@
	$(CXX) $(FLAGS) -fpic -c $(INCPATH) $< -o $@

